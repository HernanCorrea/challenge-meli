{
  "name": "challengenrwl",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "api:dev": "npx nx serve api",
    "api:test": "npx nx test api --silent",
    "api:build": "npx ng build api",
    "api:serve": "node dist/apps/api/main.js",
    "api:test-watch": "npm run test:api -- --watch",
    "client:test-e2e": "npx nx e2e client-e2e --watch",
    "client:test": "npx nx run client:test --silent --codeCoverage=true",
    "client:test-watch": "npx nx run client:test --silent --watch",
    "client:dev-ssr": "npx nx run client:serve-ssr",
    "client:build-ssr": "ng build && ng run client:server",
    "client:serve-ssr": "node dist/client/server/main.js",
    "client:prerender": "ng run client:prerender",
    "clientv2:test-e2e": "npx nx e2e clientv2-e2e --watch",
    "clientv2:dev": "npx ng s clientv2",
    "clientv2:build": "npx ng build clientv2",
    "build:all": "npm run client:test && npm run client:build-ssr && npm run api:build && npm run clientv2:build",
    "serve:production": "npm run client:serve-ssr && node dist/apps/api/main.js",
    "ng:component": "npx nx generate @nrwl/angular:component login --project=client --skip-import"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
